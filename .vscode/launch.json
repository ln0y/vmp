  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "Schema",
      "request": "launch",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "typescript-json-schema",
        "types/wx/${fileBasename}",
        "ProjectConfig",
        "-o=types/wx/debug.schema.json",
        "--required",
        "--tsNodeRegister",
        "--strictNullChecks"
      ],
      "cwd": "${workspaceFolder}/packages/schema",
      "skipFiles": [
        "<node_internals>/**"
      ],
    },
    {
      "type": "node",
      "name": "Schema2",
      "request": "launch",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "ts-json-schema-generator",
        "--path=types/wx/${fileBasename}",
        "--type=App",
        "--out=types/wx/debug.schema.json",
      ],
      "cwd": "${workspaceFolder}/packages/schema",
      "skipFiles": [
        "<node_internals>/**"
      ],
    },
  ],
}
